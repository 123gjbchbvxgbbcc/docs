{
  "redirects": [
    {
      "source": "/:path((?!zh-CN|en-US).*).html",
      "destination": "/zh-CN/:path.html"
    },
    {
      "source": "/:path((?!zh-CN|en-US).*)/",
      "destination": "/zh-CN/:path/"
    },
    {
      "source": "/",
      "destination": "/zh-CN/"
    }
  ],
  "github": {
    "silent": true
  }
}
